<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
        android:id="@+id/textViewNotFreeDoctorsSpeciality"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/spacing_major"
        android:text="Нет свободных врачей"
        android:textColor="@color/textColorPrimary"
        android:textSize="20dp"
        android:visibility="gone" />

    <ExpandableListView
        android:id="@+id/expandableListViewSpecialityDoctors"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="vertical"
        android:visibility="gone"></ExpandableListView>

    <View style="@style/AppTheme.SeparatorHorizontal" />
</LinearLayout>