package com.ibis.ibisecp2.events;

import com.ibis.ibisecp2.model.DiaryRecord;

/**
 * Created by comp on 07.04.17.
 */

public class UpdateDiaryEvent {
    private DiaryRecord mDiaryRecord;
    private int mPosition;

    public UpdateDiaryEvent(DiaryRecord diaryRecord, int position) {
        mDiaryRecord = diaryRecord;
        mPosition = position;
    }

    public DiaryRecord getDiaryRecord() {
        return mDiaryRecord;
    }

    public void setDiaryRecord(DiaryRecord diaryRecord) {
        mDiaryRecord = diaryRecord;
    }

    public int getPosition() {
        return mPosition;
    }

    public void setPosition(int position) {
        mPosition = position;
    }
}
